package com.portfolio.backend.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

@Repository
public interface PastEmployementRepository extends JpaRepository<PastEmployementRepository, Long> {

	List<PastEmployementRepository> findAllOrderByStartDate();

	List<PastEmployementRepository> findCompanyName(String name);

}
