package com.portfolio.backend.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

@Repository
public interface PastEmployement extends JpaRepository<PastEmployement, Long> {

	List<PastEmployement> findAllOrderByStartDate();

	List<PastEmployement> findCompanyName(String name);

}
